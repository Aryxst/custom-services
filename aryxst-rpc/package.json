{
 "name": "aryxst-rpc",
 "type": "module",
 "description": "A custom discord RPC app",
 "scripts": {
  "start": "tailwindcss -i ./public/css/global.css -o ./public/css/global.min.css && node src/app.js",
  "watch:css": "tailwindcss -i ./public/css/global.css -o ./public/css/global.min.css --watch",
  "dev": "concurrently --kill-others-on-fail \"nodemon src/app.js --ignore generated/\" \"pnpm run watch:css\""
 },
 "dependencies": {
  "discord-rpc": "^4.0.1",
  "express": "5.0.1",
  "ps-list": "^8.1.1",
  "squirrelly": "^9.1.0"
 },
 "devDependencies": {
  "@catppuccin/tailwindcss": "^0.1.6",
  "@egoist/tailwindcss-icons": "^1.8.2",
  "@iconify-json/lucide": "^1.2.18",
  "@types/discord-rpc": "^4.0.8",
  "@types/express": "^5.0.0",
  "@types/node": "^20.17.10",
  "concurrently": "^9.1.0",
  "nodemon": "^3.1.9",
  "tailwindcss": "^3.4.16"
 }
}
